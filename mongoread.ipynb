ll_type": "code", "metadata": {"id": "129794075_0.844497970035804"}, "execution_count": null, "source": ["import paramiko\n\n\ndef read_sftp(hostname,port,username,password):\n    ssh_client = paramiko.SSHClient()\n    ssh_client.set_missing_host_key_policy(paramiko.AutoAddPolicy())\n    ssh_client.connect(hostname=hostname, port=port, username=username, password=password)\n    sftp_client = ssh_client.open_sftp()\n    remote_directory_path = '/home/shabnam/source'  \n    sftp_client.chdir(remote_directory_path)\n    file_list = sftp_client.listdir()\n\n    # Print the list of files\n    print(\"List of files in the remote directory:\")\n    for file_name in file_list:\n        logging.info(file_name)\n        print(file_name)\n\n    sftp_client.close()\n    ssh_client.close()\n"], "outputs": []}, {"cell_type": "code", "metadata": {"id": "129794075_0.3118582185626195"}, "execution_count": null, "source": ["read_sftp('172.21.85.186',22,'shabnam','Sh173$2763!')"], "outputs": []}, {"cell_type": "code", "metadata": {"id": "129794075_0.8099011569026553"}, "execution_count": 4, "source": ["from python_files import add\ndef sum(a,b,c):\n    res=add.start(a,b,c)\n    return res"], "outputs": []}, {"cell_type": "code", "metadata": {"id": "129794075_0.7972002728534273"}, "execution_count": 5, "source": ["a=2\nb=3\nc=5\nsum(a,b,c)"], "outputs": [{"data": {"text/plain": "10"}, "metadata": {}, "execution_count": 6, "output_type": "execute_result"}]}, {"cell_type": "code", "metadata": {"id": "129794075_0.03569937141300161"}, "execution_count": 8, "source": ["import boto3\nimport pandas as pd\nfrom io import BytesIO\nimport pymongo\nfrom pymongo import MongoClient\n\n\n\ndef read_s3_data(aws_access_key_id,aws_secret_access_key,bucket_name,prefix,conn_str,database,collection):\n    client = MongoClient(conn_str)\n    db = client[database] \n    collection = db[collection]\n    s3_client = boto3.client('s3', aws_access_key_id=aws_access_key_id,\n                            aws_secret_access_key=aws_secret_access_key)\n\n\n    objects = s3_client.list_objects(Bucket=bucket_name, Prefix=prefix)\n\n\n    for obj in objects.get('Contents', []):\n        file_key = obj['Key']\n\n\n        if not file_key.endswith('/'):\n\n            response = s3_client.get_object(Bucket=bucket_name, Key=file_key)\n            file_content = response['Body'].read()\n\n\n            df = pd.read_csv(BytesIO(file_content))\n            records = df.to_dict(orient='records')\n            \n            collection.insert_many(records)\n            print(f\"Inserted {df.shape[0]} rows  into MongoDB\")\n            logging.info(f\"Inserted {df.shape[0]} rows  into MongoDB\")\n\n            print(f\"File: {file_key}, Shape: {df.shape}\")\n            logging.info(f\"File: {file_key}, Shape: {df.shape}\")\n            \ndef funs():\n    return \"Yes\"\n"], "outputs": []}, {"cell_type": "code", "metadata": {"id": "129794075_0.4859043052418228"}, "execution_count": 9, "source": ["aws_access_key_id = ''\naws_secret_access_key = ''\nbucket_name = 'sandeep0924'\nprefix = 'jobs/'\nconn_str=''\ndatabase='pipeline'\ncollection='python_ondemandjob'\n\nread_s3_data(aws_access_key_id,aws_secret_access_key,bucket_name,prefix,conn_str,database,collection)"], "outputs": [{"name": "stdout", "text": "Inserted 312 rows  into MongoDB\nFile: jobs/ApportionedIdentifiers.csv, Shape: (312, 15)\nInserted 1000 rows  into MongoDB\nFile: jobs/accounts.csv, Shape: (1000, 16)\nInserted 5498 rows  into MongoDB\nFile: jobs/census_2011.csv, Shape: (5498, 33)\nInserted 100 rows  into MongoDB\nFile: jobs/emp.csv, Shape: (100, 11)\nInserted 10 rows  into MongoDB\nFile: jobs/s3_test.csv, Shape: (10, 12)\nInserted 50000 rows  into MongoDB\nFile: jobs/small_table_202401192033.csv, Shape: (50000, 5)\n", "output_type": "stream"}]}], "metadata": {}, "nbformat": 4, "nbformat_minor": 2}
